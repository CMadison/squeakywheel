<h1>Create a Complaint</h1>

<%= form_for(:complaint) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.label :screenshot %>
  <%= f.file_field :screenshot %>

  <%= f.label :company_id %>
  <%= f.number_field :company_id %>

  <%= f.submit 'Create' %>
<% end %>